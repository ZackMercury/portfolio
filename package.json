{
  "name": "portfolio",
  "version": "0.1.0",
  "description": "My portfolio website",
  "main": "./src/main.ts",
  "scripts": {
    "build": "webpack --mode production && copy \"%cd%\\src\\index.html\" \"%cd%\\dist\\index.html\" /a && copy \"%cd%\\src\\bundle.css\" \"%cd%\\dist\\bundle.css\" /a && xcopy \"%cd%\\src\\img\" \"%cd%\\dist\\img\" /E /H /C /I /Y && explorer \"%cd%\\dist\\index.html\" & exit 0",
    "watch": "webpack --mode production --watch && copy \"%cd%\\src\\index.html\" \"%cd%\\dist\\index.html\" /a && copy \"%cd%\\src\\bundle.css\" \"%cd%\\dist\\bundle.css\" /a",
    "start": "explorer \"%cd%\\dist\\index.html\" & exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZackMercury/portfolio.git"
  },
  "author": "ZackMercury",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZackMercury/portfolio/issues"
  },
  "homepage": "https://github.com/ZackMercury/portfolio#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "babel-loader": "^8.2.3",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {}
}
